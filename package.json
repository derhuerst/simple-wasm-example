{
	"private": true,
	"name": "simple-wasm-example",
	"description": "A Rust lib, compiled to WASM, with fallback to asm.js.",
	"version": "0.1.0",
	"main": "index.js",
	"files": [
		"index.js",
		"src",
		"target/asmjs-unknown-emscripten/release/hello_world.js",
		"target/wasm32-unknown-emscripten/release/hello_world.js",
		"target/wasm32-unknown-emscripten/release/hello_world.wasm"
	],
	"keywords": [
		"wasm",
		"rust",
		"asm.js",
		"webassembly"
	],
	"author": "Jannis R <mail@jannisr.de>",
	"homepage": "https://github.com/derhuerst/simple-wasm-example",
	"repository": "derhuerst/simple-wasm-example",
	"bugs": "https://github.com/derhuerst/simple-wasm-example/issues",
	"license": "ISC",
	"engines": {
		"node": ">=6"
	},
	"dependencies": {
	},
	"devDependencies": {
	},
	"scripts": {
		"build:dev:asm": "cargo build --target asmjs-unknown-emscripten",
		"build:dev:wasm": "cargo build --target wasm32-unknown-emscripten",
		"build:asm": "cargo build --target asmjs-unknown-emscripten --release",
		"build:wasm": "cargo build --target wasm32-unknown-emscripten --release",
		"build:dev": "npm run build:dev:asm && npm run build:dev:wasm",
		"build": "npm run build:asm && npm run build:wasm",
		"prepublishOnly": "npm test"
	}
}
